/**
 * 16-bit bitwise Xor:
 * for i = 0..15: out[i] = (a[i] Xor b[i])
 */

CHIP Xor16One {
    IN a[16], b;
    OUT out[16];

    PARTS:
    // Put your code here:
	Xor(a = a[0], b = b, out = out[0]);
	Xor(a = a[1], b = b, out = out[1]);
	Xor(a = a[2], b = b, out = out[2]);
	Xor(a = a[3], b = b, out = out[3]);
	Xor(a = a[4], b = b, out = out[4]);
	Xor(a = a[5], b = b, out = out[5]);
	Xor(a = a[6], b = b, out = out[6]);
	Xor(a = a[7], b = b, out = out[7]);
	Xor(a = a[8], b = b, out = out[8]);
	Xor(a = a[9], b = b, out = out[9]);
	Xor(a = a[10], b = b, out = out[10]);
	Xor(a = a[11], b = b, out = out[11]);
	Xor(a = a[12], b = b, out = out[12]);
	Xor(a = a[13], b = b, out = out[13]);
	Xor(a = a[14], b = b, out = out[14]);
	Xor(a = a[15], b = b, out = out[15]);
}